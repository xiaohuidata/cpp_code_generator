#pragma once

#define CPP_CODE_GENERATOR_VERSION_MAJOR @PROJECT_VERSION_MAJOR@
#define CPP_CODE_GENERATOR_VERSION_MINOR @PROJECT_VERSION_MINOR@
#define CPP_CODE_GENERATOR_VERSION_PATCH @PROJECT_VERSION_PATCH@
#define CPP_CODE_GENERATOR_VERSION "@PROJECT_VERSION@"

#define CPP_CODE_GENERATOR_VERSION_CHECK(major, minor, patch) \
    (CPP_CODE_GENERATOR_VERSION_MAJOR > major || \
     (CPP_CODE_GENERATOR_VERSION_MAJOR == major && \
      (CPP_CODE_GENERATOR_VERSION_MINOR > minor || \
       (CPP_CODE_GENERATOR_VERSION_MINOR == minor && \
        CPP_CODE_GENERATOR_VERSION_PATCH >= patch))))
